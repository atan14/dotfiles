{
  "keys": {
    "command": {
      "bind": {
        "&-&": "jupyter-notebook:shutdown-kernel",
        "shift-s,shift-s": "jupyter-notebook:shutdown-kernel",
        "0,1": "jupyter-notebook:restart-kernel-and-run-all-cells"
      }
    }
  },
  "load_extensions": {
    "nglview": true,
    "nglview-js-widgets/extension": true
  },
  "MarkdownCell": {
    "cm_config": {
      "lineWrapping": true
    }
  },
  "CodeCell": {
    "cm_config": {
      "lineWrapping": true
    }
  }
}
